<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dottec.pdi.project.pdi.viewmodel.AddGoalFromTemplateViewModel">
    <padding>
        <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
    </padding>
    <children>
        <VBox fx:id="goalTemplatesVBox" maxWidth="700.0" spacing="10.0" style="-fx-background-color: #F6F6F6; -fx-background-radius: 10;" VBox.vgrow="ALWAYS">
            <effect>
                <DropShadow color="#00000040" />
            </effect>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
        </VBox>
    </children>
   <stylesheets>
      <URL value="@../static/style/style_GoalTemplates.css" />
      <URL value="@../static/style/style_template.css" />
   </stylesheets>
</VBox>
